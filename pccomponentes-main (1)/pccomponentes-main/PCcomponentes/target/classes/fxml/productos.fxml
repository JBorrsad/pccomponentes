          <TableColumn fx:id="IDTABLA" prefWidth="94.0" text="ID" />
            <TableColumn fx:id="NOMBRETABLA" prefWidth="186.0" text="Nombre" />
            <TableColumn fx:id="STOCKTABLA" prefWidth="162.0" text="Stock" />
            <TableColumn fx:id="PRECIOTABLA" prefWidth="158.0" text="Precio" />
        </columns>
      </TableView>
      <Button fx:id="refrescar" layoutX="80.0" layoutY="385.0" mnemonicParsing="false" onAction="#refrescar" text="refrescar" />
       <Button fx:id="añadirstock" layoutX="205.0" layoutY="385.0" mnemonicParsing="false" text="añadir stock" />
       <Button fx:id="eliminar" layoutX="505.0" layoutY="385.0" mnemonicParsing="false" onAction="#eliminar" text="eliminar" />
      <Button fx:id="modificar" layoutX="345.0" layoutY="385.0" mnemonicParsing="false" onAction="#abrirVentanaModificar" text="modificar" />
   </children>
</AnchorPane>
